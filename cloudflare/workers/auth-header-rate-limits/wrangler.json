{
	"name": "auth-header-rate-limits",
	"main": "src/index.ts",
	"compatibility_date": "2025-01-28",
	"unsafe": {
		"bindings": [
			{
				"name": "MY_RATE_LIMITER",
				"type": "ratelimit",
				"namespace_id": "1001",
				"simple": {
					"limit": 100,
					"period": 10
				}
			}
		]
	},
	"routes": [
		{
			"pattern": "api.chunkr.ai/*",
			"zone_name": "chunkr.ai"
		}
	],
	"logpush": true
}